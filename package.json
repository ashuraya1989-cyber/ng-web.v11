{
  "name": "nishagoriel-photography",
  "version": "1.0.0",
  "description": "Modern photography portfolio website with admin panel",
  "private": true,
  "scripts": {
    "start": "concurrently -n \"BACKEND,FRONTEND\" -c \"cyan,magenta\" \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "node scripts/start-backend.js",
    "start:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install:all": "npm run install:frontend && npm run install:backend",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "node scripts/install-backend.js",
    "test": "cd backend && python -m pytest tests/ || python3 -m pytest tests/",
    "setup": "echo Please run setup.bat (Windows) or ./setup.sh (Linux/Mac)"
  },
  "keywords": [
    "photography",
    "portfolio",
    "react",
    "fastapi",
    "supabase",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
